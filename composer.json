{
    "name": "helhum/typo3-crontab",
    "type": "typo3-cms-extension",
    "description": "Advanced scheduling for TYPO3 Console commands and TYPO3 Scheduler tasks",
    "homepage": "https://typo3.org",
    "license": [
        "GPL-2.0-or-later"
    ],
    "authors": [
        {
            "name": "Helmut Hummel",
            "email": "info@helhum.io",
            "role": "Developer"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "typo3/cms-core": "~9.5.0"
    },
    "suggest": {
        "typo3/cms-scheduler": "For executing scheduler tasks"
    },
    "conflict": {
        "typo3/cms": "*"
    },
    "replace": {
        "crotab": "*"

    },
    "extra": {
        "typo3/cms": {
            "extension-key": "crontab"
        }
    },
    "autoload": {
        "psr-4": {
            "Helhum\\TYPO3\\Crontab\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Helhum\\TYPO3\\Crontab\\Tests\\": "Tests/"
        }
    }
}
